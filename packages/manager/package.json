{
  "name": "@philter/manager",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:60080",
  "scripts": {
    "build": "tsc && vite build",
    "dev": "vite",
    "lint": "eslint .",
    "serve": "vite preview"
  },
  "eslintConfig": {
    "extends": [
      "plugin:react/recommended",
      "plugin:react-hooks/recommended"
    ],
    "plugins": [
      "react"
    ],
    "rules": {
      "react-hooks/exhaustive-deps": [
        "warn",
        {
          "additionalHooks": "(useAsync|useAsyncCallback)"
        }
      ]
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    }
  },
  "engines": {
    "node": ">=12"
  },
  "dependencies": {
    "@blueprintjs/core": "^3.44.0",
    "@blueprintjs/popover2": "^0.9.0",
    "@philter/common": "*",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-virtualized-auto-sizer": "^1.0.0",
    "@types/react-window": "^1.8.2",
    "classnames": "^2.3.1",
    "dequal": "^2.0.2",
    "react": "^17.0.2",
    "react-async-hook": "^3.6.2",
    "react-dom": "^17.0.2",
    "react-virtualized-auto-sizer": "^1.0.5",
    "react-window": "^1.8.6",
    "swr": "^0.5.5",
    "typescript": "^4.2.4"
  },
  "devDependencies": {
    "@vitejs/plugin-react-refresh": "^1.3.3",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "gts": "^3.1.0",
    "rollup-plugin-delete": "^2.0.0",
    "vite": "^2.3.2"
  }
}
